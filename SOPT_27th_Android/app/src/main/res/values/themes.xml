<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Semina" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/black</item>
        <item name="colorPrimaryVariant">@color/yellowFFCE05</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimary</item>
        <!-- Customize your theme here. -->
        <!--Activity-->
        <item name="android:windowBackground">@drawable/login_activitiy</item>
        <item name="fontFamily">@font/spoqa_han_sans</item>
        <!--default Text Setting-->
        <item name="android:textColor">@color/white</item>
        <item name="android:textColorHint">@color/grayC4</item>
        <item name="android:editTextBackground">@android:color/transparent</item>
        <item name="android:editTextColor">@color/white</item>

    </style>


</resources>